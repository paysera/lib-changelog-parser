{
    "name": "paysera/lib-changelog-parser",
    "description": "Parses changelog.md formatted by keepachangelog.com to PHP structure and dumps back",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Paysera\\Component\\ChangelogParser\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paysera\\Component\\ChangelogParser\\Tests\\":"tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "twig/twig": "^1.0 | ^2.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    }
}
